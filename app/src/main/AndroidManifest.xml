<manifest package="com.bleyl.pictorial"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".App"
        android:fullBackupContent="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".view.MainActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="www.imgur.com"
                    android:pathPattern=".*\/.*"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPattern=".*\/.*"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPattern=".*\/.*"
                    android:scheme="https" />
                <data
                    android:host="www.imgur.com"
                    android:pathPattern=".*\/.*"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="www.i.imgur.com"
                    android:pathPattern=".*\\..*"
                    android:scheme="http" />
                <data
                    android:host="i.imgur.com"
                    android:pathPattern=".*\\..*"
                    android:scheme="http" />
                <data
                    android:host="i.imgur.com"
                    android:pathPattern=".*\\..*"
                    android:scheme="https" />
                <data
                    android:host="www.i.imgur.com"
                    android:pathPattern=".*\\..*"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:host="www.gfycat.com"
                    android:scheme="http" />
                <data
                    android:host="gfycat.com"
                    android:scheme="http" />
                <data
                    android:host="gfycat.com"
                    android:scheme="https" />
                <data
                    android:host="www.gfycat.com"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="www.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="https" />
                <data
                    android:host="www.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="www.m.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="http" />
                <data
                    android:host="m.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="http" />
                <data
                    android:host="m.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="https" />
                <data
                    android:host="www.m.imgur.com"
                    android:pathPrefix="/gallery/"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="www.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="http" />
                <data
                    android:host="imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="https" />
                <data
                    android:host="www.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="www.m.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="http" />
                <data
                    android:host="m.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="http" />
                <data
                    android:host="m.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="https" />
                <data
                    android:host="www.m.imgur.com"
                    android:pathPrefix="/a/"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:host="*"
                      android:scheme="http"
                      android:pathPattern=".*\\.png" />
                <data android:host="*"
                      android:scheme="https"
                      android:pathPattern=".*\\.png" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:host="*"
                      android:scheme="http"
                      android:pathPattern=".*\\.jpg" />
                <data android:host="*"
                      android:scheme="https"
                      android:pathPattern=".*\\.jpg" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:host="*"
                      android:scheme="http"
                      android:pathPattern=".*\\.jpeg" />
                <data android:host="*"
                      android:scheme="https"
                      android:pathPattern=".*\\.jpeg" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:host="*"
                      android:scheme="http"
                      android:pathPattern=".*\\.bmp" />
                <data android:host="*"
                      android:scheme="https"
                      android:pathPattern=".*\\.bmp" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:host="*"
                      android:scheme="http"
                      android:pathPattern=".*\\.gif" />
                <data android:host="*"
                      android:scheme="https"
                      android:pathPattern=".*\\.gif" />
            </intent-filter>
        </activity>

        <service android:name=".view.MainService"
                 android:launchMode="singleTask"/>

    </application>

</manifest>
